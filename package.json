{
    "name": "angular-workspace",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "start:app-app1": "ng serve app-app1",
        "start:app-app2": "ng serve app-app2",
        "start:app-shell": "ng serve app-shell -o",
        "build:lib-micro-front-end": "ng build lib-micro-front-end",
        "build:app-app1": "ng build app-app1",
        "build:app-app2": "ng build app-app2",
        "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
        "build:lib-micro-front-end-ci": "ng build lib-micro-front-end",
        "test:lib-micro-front-end-ci": "ng test lib-micro-front-end --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCI",
        "build:app-app1-ci": "ng build app-app1",
        "test:app-app1-ci": "ng test app-app1 --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCI",
        "build:app-app2-ci": "ng build app-app2",
        "test:app-app2-ci": "ng test app-app2 --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCI",
        "build:app-shell-ci": "ng build app-app2",
        "test:app-shell-ci": "ng test app-shell --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCI"
    },
    "private": true,
    "dependencies": {
        "@angular-architects/module-federation": "^14.2.3",
        "@angular-architects/module-federation-tools": "^14.2.3",
        "@angular/animations": "~13.3.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/elements": "^13.3.5",
        "@angular/forms": "~13.3.0",
        "@angular/localize": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "@jamarsto/kiunzi-micro-frontend-tools": "^0.0.32",
        "@ng-bootstrap/ng-bootstrap": "^12.1.1",
        "@popperjs/core": "^2.10.2",
        "angular-auth-oidc-client": "14.0.0",
        "bootstrap": "^5.1.3",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.5",
        "@angular/cli": "~13.3.5",
        "@angular/compiler-cli": "~13.3.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ng-packagr": "^13.0.0",
        "ngx-build-plus": "^13.0.1",
        "typescript": "~4.6.2"
    }
}